{
  "schemaVersion": 1,
  "id": "spu",
  "version": "${version}",
  "name": "Server Pack Unlocker",
  "description": "Allows resource packs provided by servers to be overridden and moved. This is useful for servers that force the client to use a resource pack.",
  "authors": [
    "TheMysterys"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/server-pack-unlocker",
    "issues": "https://github.com/TheMysterys/Server-Pack-Unlocker/issues",
    "sources": "https://github.com/TheMysterys/Server-Pack-Unlocker"
  },
  "license": "Public Domain",
  "icon": "assets/spu/icon.png",
  "environment": "client",
  "mixins": [
    "spu.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.17 <=1.20.1"
  }
}
